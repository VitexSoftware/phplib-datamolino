{
    "name": "datamolino",
    "description": "Datamolino Client Library",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "version": "0.1",
    "type": "library",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "require": {
        "php": ">= 5.6",
        "ext-curl": "*",
        "ease-framework": "*"
    },
    "autoload": {
        "psr-4": {
            "Datamolino\\": "./"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\Datamolino\\": "tests/src/Datamolino/",
            "Test\\": "testing/"
        }
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.6.0",
        "ext-curl": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "/usr/share/php/Ease",
            "options": {
                "symlink": true
            }
        }
    ]
}
