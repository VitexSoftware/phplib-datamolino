{
    "name": "vitexsoftware/datamolino",
    "description": "Datamolino Client Library",
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Vítězslav Dvořák",
            "email": "info@vitexsoftware.cz"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "vitexsoftware/ease-framework": "*",
        "jakub-onderka/php-parallel-lint": "*",
        "jakub-onderka/php-console-highlighter": "*"
    },
    "autoload": {
        "psr-4": {
            "Datamolino\\": "src/Datamolino/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\Ease\\": "vendor/vitexsoftware/ease-framework/tests/src/Ease",
            "Test\\Datamolino\\": "tests/src/Datamolino/",
            "Test\\": "testing/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "6.0.x-dev",
        "phpunit/php-code-coverage": "^5.0"
    }
}
